{
  "id": null,
  "title": "Data Quality Overview",
  "timezone": "browser",
  "refresh": "10s",
  "schemaVersion": 37,
  "version": 1,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "PROM"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "lines",
            "lineWidth": 2
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 latency",
          "refId": "A"
        }
      ],
      "title": "API Latency (p95)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "PROM"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "targets": [
        {
          "expr": "sum by (severity) (rate(incidents_total[5m]))",
          "legendFormat": "{{severity}}",
          "refId": "A"
        }
      ],
      "title": "Incidents by Severity",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "PROM"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "targets": [
        {
          "expr": "sum by (dataset) (rate(checks_run_total[5m]))",
          "legendFormat": "{{dataset}}",
          "refId": "A"
        }
      ],
      "title": "Checks Run per Dataset",
      "type": "timeseries"
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}